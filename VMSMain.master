            background-image: none;
            width: 229px;
            margin-bottom: 8px;
            margin-top: 0%;
            margin-left: 20px;
            height: 33px;
        }

        span.k-timepicker {
            background-color: transparent;
            background-image: none;
            width: 105px;
            margin-bottom: 8px;
            margin-top: 1%;
            margin-left: 16px;
            height: 38px;
        }

        .k-list {
            color: #596679;
            font: 14px Proxima-Nova-Regular;
            background-color: #ffffff;
        }

        .tooltip {
            position: relative;
            display: inline-block;
        }

            .tooltip .tooltiptext {
                visibility: hidden;
                width: 97px;
                background-color: #555;
                color: #fff;
                text-align: center;
                border-radius: 6px;
                padding: 5px 0;
                position: absolute;
                z-index: 1;
                bottom: 125%;
                left: 50%;
                margin-left: -56px;
                opacity: 0;
                transition: opacity 0.3s;
            }

                .tooltip .tooltiptext::after {
                    content: "";
                    position: absolute;
                    top: 100%;
                    left: 50%;
                    margin-left: -5px;
                    border-width: 5px;
                    border-style: solid;
                    border-color: #555 transparent transparent transparent;
                }

            .tooltip:hover .tooltiptext {
                visibility: visible;
                opacity: 1;
            }

        .ui-dialog .ui-dialog-title {
            margin-top: 2%;
        }

        .k-calendar .k-header .k-link {
