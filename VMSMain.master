        .k-picker-wrap.k-state-default {
            border-color: #CCCCCC;
            height: 36px;
            margin-left: 0%;
            margin-top: 4px;
            border: 1px solid #cccccc;
            border-top: 0;
            border-left: 0;
            border-right: 0;
        }

        .k-picker-wrap.k-state-disabled {
            border-color: #CCCCCC;
            height: 36px;
            margin-left: -4%;
            margin-top: 4px;
            border: 1px solid #cccccc;
            border-top: 0;
            border-left: 0;
            border-right: 0;
        }

        .k-state-default > .k-select {
            border-color: transparent;
            background-color: transparent;
            background-image: none;
            line-height: 45px;
            top: 10px;
            right: 2px;
        }

        .k-state-disabled > .k-select {
            border-color: transparent;
            background-color: transparent;
            background-image: none;
            line-height: 45px;
            top: 10px;
        }
