        /// <summary>
        /// The current date field
        /// </summary>        
        private static DateTime crntdate;

        /// <summary>
        /// Gets or sets current date value
        /// </summary>
[SuppressMessage("Microsoft.Usage", "CA1801:ReviewUnusedParameters", Justification = "Reviewed")]
        public static DateTime CURRENTDATE
        {
            get
            {
                return crntdate;
            }

            set
            {
                crntdate = DateTime.Now;
            }
        }

        /// <summary>
        /// Gets or sets from time now value
        /// </summary>
[SuppressMessage("Microsoft.Usage", "CA1801:ReviewUnusedParameters", Justification = "Reviewed")]
        public static string FROMTIMENV
        {
            get
            {
                return FROMTIMENV;
            }

            set
            {
                FROMTIMENV = DateTime.Now.ToShortTimeString();
            }
        }

        /// <summary>
        /// Gets or sets to time now value
        /// </summary>
[SuppressMessage("Microsoft.Usage", "CA1801:ReviewUnusedParameters", Justification = "Reviewed")]
        public static string TOTIMENV
        {
            get
            {
                return TOTIMENV;
            }

            set
            {
                TOTIMENV = DateTime.Now.AddHours(3).ToShortTimeString();
            }
        }
    }
}
